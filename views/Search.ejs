<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="="IE=edge>
    <meta name="viewport" content="width=device-width, initial-scal=1.0">
    <title>Restaurant Ranking</title>

    <style>
        table {
          font-family: optima, sans-serif;
          border-collapse: collapse;
          width: 100%;
        }
  
        td,tr,th {
          border:  1px solid #dddddd;
          text-align: center;
          padding: 8px;
        }
  
        tr:nth-child(even) {
          background-color: #f2f2f2;
        }
  
        .item {
          cursor: pointer;
          transition: 0.3s;
        }
  
        .item:hover {
          transform: scale(1.07);
        }
      </style>

    <link rel="canonical" href="https://getbootstrap.com/docs/4.0/examples/pricing/">

    <link href="../../dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="pricing.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  
</head>

<body>
    <div class="d-flex flex-column flex-md-row align-items-center p-5 px-md-4 mb-3 bg-white border-bottom box-shadow">
        <nav class="navbar navbar-expand-md navbar-dark bg-dark fixed-top">
            <a class="navbar-brand" href="/" style="font-weight: 700;">Restaurant Ranking</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
      
            <div class="collapse navbar-collapse" id="navbarsExampleDefault">
              <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                  <a class="nav-link" href="Category.ejs">Category</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="Sales.ejs">Sales</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="Franchise.ejs">Franchise</a>
                </li>
                <!-- <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle" href="http://example.com" id="dropdown01" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Dropdown</a>
                  <div class="dropdown-menu" aria-labelledby="dropdown01">
                    <a class="dropdown-item" href="#">Action</a>
                    <a class="dropdown-item" href="#">Another action</a>
                    <a class="dropdown-item" href="#">Something else here</a>
                  </div>
                </li> -->
              </ul>
              <form class="form-inline my-2 my-lg-0" action="Search.ejs" method="POST">
                <lable>
                  <span></span>
                    <input class="form-control mr-sm-2" type="text" name="search" placeholder="Search Restaurant Name" >
                  </span>
                  <button class="btn btn-outline-success my-2 my-sm-0" type="submit" value="Search">Search</button>
                </lable>
              </form>
            </div>
        </nav>
    </div>
    <br><br><br><br>
    <center>
      <h1 style="font-weight: 700;">Result</h1>
    </center>

    <main role="main">
        <div class="container marketing">
            <center>
                <br>
                <table>
                  <thead>
                    <tr>
                      <th>Rank</th>
                      <th>Restaurant Name</th>
                      <th>Restaurant Type</th>
                      <th>Sales</th>
                      <th>YOY Sales</th>
                      <th>Units</th>
                      <th>YOY Units</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% result.forEach(function(row) { %>
                      <tr>
                        <td><%= row.Rank %></td>
                        <td><%= row.RestaurantName %></td>
                        <td><%= row.RestaurantType %></td>
                        <td><%= row.Sales %></td>
                        <td><%= row.YOY_Sales %></td>
                        <td><%= row.Units %></td>
                        <td><%= row.YOY_Units %></td>
                      </tr>
                    <% }) %>
                  </tbody>
                </table>
            </center>
            <br><br>
        </div>
        <center>
            <p><a class="btn btn-secondary" href="/" role="button">&laquo; Go Back</a></p>
        </center>
    </main>
</body>
</html>